clean:
	@${MAKE} --no-print-directory -C . -f kmeter_standalone_stereo.make clean
	@${MAKE} --no-print-directory -C . -f kmeter_standalone_surround.make clean
	@${MAKE} --no-print-directory -C . -f kmeter_vst_stereo.make clean
